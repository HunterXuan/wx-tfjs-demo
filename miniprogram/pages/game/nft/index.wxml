<!--pages/basic/posenet/index.wxml-->
<custom-header bgColor="bg-gradual-blue" isBack="{{true}}">
  <view slot="backText">返回</view>
  <view slot="content">目标追踪</view>
</custom-header>
<camera
  device-position="back"
  flash="off"
  binderror="error"
  frame-size="medium"
  class="camera"
  style="height:{{cameraBlockHeight}}px;"
>
  <!--<canvas id="canvas" canvas-id="canvas" class="canvas"></canvas>-->
  <canvas class="webgl" type="webgl" id="gl" bindtouchstart="onTX" bindtouchmove="onTX" bindtouchend="onTX"></canvas>
</camera>